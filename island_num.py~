import pdb

class Solution(object):

    def __init__(self):
        self.visited = [[False]*X]*Y

    def island_num(self, matrix):
        X = len(matrix[0])
        Y = len(matrix)

        for i in range(X):
            for j in range(Y):
                if visited[i][j] == False and matrix[i][j] == 1:
                    self.expand(i, j)


    def expand(self, i, j):
        queue = []
        queue.push((i,j))
        self.visited[i][j]
        while len(queue) > 0:
            
        
